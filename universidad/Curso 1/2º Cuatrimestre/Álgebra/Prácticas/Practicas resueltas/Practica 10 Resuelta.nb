Cell[CellGroupData[{

Cell["d) Calcular una base ortonormal", "Subsection",
 CellChangeTimes->{{3.733723325404436*^9, 
  3.733723330308717*^9}},ExpressionUUID->"76fe11f1-cd4c-489e-84a8-\
ef0a8b89e198"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Bortog2", "[", 
   RowBox[{"A_", ",", " ", "B_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"n", ",", " ", "e"}], "}"}], ",", "\n", "\t\t\t", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"n", "=", 
        RowBox[{"Length", "[", "B", "]"}]}], ";"}], "\n", "\t\t\t", 
      RowBox[{
       RowBox[{"e", "=", 
        RowBox[{"Table", "[", 
         RowBox[{"0", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";"}], "\n", 
      "\t\t\t", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", " ", 
        RowBox[{"i", "<=", "n"}], ",", " ", 
        RowBox[{"i", " ", "=", " ", 
         RowBox[{"i", "+", "1"}]}], ",", " ", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"i", "<=", "1"}], ",", " ", 
          RowBox[{
           RowBox[{"e", "[", 
            RowBox[{"[", "i", "]"}], "]"}], " ", "=", " ", 
           RowBox[{"B", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], ",", " ", 
          RowBox[{
           RowBox[{"e", "[", 
            RowBox[{"[", "i", "]"}], "]"}], " ", "=", " ", 
           RowBox[{
            RowBox[{"B", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "-", "\n", "     ", 
            RowBox[{"Sum", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"B", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], ".", "A", ".", 
                   RowBox[{"e", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}], ")"}], "/", 
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"e", "[", 
                    RowBox[{"[", "j", "]"}], "]"}], ".", "A", ".", 
                   RowBox[{"e", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}], ")"}]}], ")"}], "*", 
               RowBox[{"e", "[", 
                RowBox[{"[", "j", "]"}], "]"}]}], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", "1", ",", 
                RowBox[{"i", "-", "1"}]}], "}"}]}], "]"}]}]}]}], "]"}]}], 
       "]"}]}], ";", "\n", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<La base ortogonal es\>\"", ",", "e"}], "]"}], ";", "\n", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<La base ortonormal es\>\"", ",", "  ", "\n", "      ", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"e", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "/", 
          RowBox[{"Sqrt", "[", 
           RowBox[{
            RowBox[{"e", "[", 
             RowBox[{"[", "i", "]"}], "]"}], ".", "A", ".", 
            RowBox[{"e", "[", 
             RowBox[{"[", "i", "]"}], "]"}]}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}]}], "]"}]}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.7339866447043695`*^9, 
  3.7339866455275335`*^9}},ExpressionUUID->"9759458d-6bc9-45a0-9970-\
a6da9a7c654f"],


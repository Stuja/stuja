MVI A, 00;
MVI B, 00;
MVI C, 00;
MVI D, 00;
MVI E, 00;
MVI H, 00;
MVI L, 00;


IN 00;
MOV e, a;

LXI B, 00FF;
CALL SUMALISTA;
CALL DIVISION;
MOV a, d;
OUT 02;
hlt;



Cell[BoxData[
 RowBox[{
  RowBox[{"CHINO", "[", 
   RowBox[{"a_", ",", "m_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"n", "=", 
      RowBox[{"Length", "[", "a", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"tabla", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"n", "+", "1"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "7"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"tabla", "[", 
       RowBox[{"[", "1", "]"}], "]"}], "=", 
      RowBox[{"{", 
       RowBox[{
       "\"\<i\>\"", ",", "\"\<\\!\\(a\\_i\\)\>\"", ",", 
        "\"\<\\!\\(m\\_i\\)\>\"", ",", "\"\<\\!\\(M\\_i\\)\>\"", ",", 
        "\"\<\\!\\(u\\_i\\)\>\"", ",", "\"\<\\!\\(w\\_i\\)\>\"", ",", 
        "\"\<\\!\\(b\\_i\\)\>\""}], "}"}]}], ";", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"k", "=", "2"}], ",", "\[IndentingNewLine]", 
       RowBox[{"k", "\[LessEqual]", 
        RowBox[{"n", "+", "1"}]}], ",", 
       RowBox[{"k", "++"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"tabla", "[", 
          RowBox[{"[", 
           RowBox[{"k", ",", "1"}], "]"}], "]"}], "=", 
         RowBox[{"k", "-", "1"}]}], ";", 
        RowBox[{
         RowBox[{"tabla", "[", 
          RowBox[{"[", 
           RowBox[{"k", ",", "2"}], "]"}], "]"}], "=", 
         RowBox[{"a", "[", 
          RowBox[{"[", 
           RowBox[{"k", "-", "1"}], "]"}], "]"}]}], ";", 
        RowBox[{
         RowBox[{"tabla", "[", 
          RowBox[{"[", 
           RowBox[{"k", ",", "3"}], "]"}], "]"}], "=", 
         RowBox[{"m", "[", 
          RowBox[{"[", 
           RowBox[{"k", "-", "1"}], "]"}], "]"}]}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"M", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"M", "[", 
       RowBox[{"[", "1", "]"}], "]"}], "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"tabla", "[", 
       RowBox[{"[", 
        RowBox[{"2", ",", "4"}], "]"}], "]"}], "=", "1"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"f", "=", "2"}], ",", 
       RowBox[{"f", "<", 
        RowBox[{"n", "+", "1"}]}], ",", 
       RowBox[{"f", "++"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"M", "[", 
          RowBox[{"[", "f", "]"}], "]"}], "=", 
         RowBox[{
          RowBox[{"M", "[", 
           RowBox[{"[", 
            RowBox[{"f", "-", "1"}], "]"}], "]"}], "*", 
          RowBox[{"m", "[", 
           RowBox[{"[", 
            RowBox[{"f", "-", "1"}], "]"}], "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"tabla", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{"f", "+", "1"}], ",", "4"}], "]"}], "]"}], "=", 
         RowBox[{"M", "[", 
          RowBox[{"[", "f", "]"}], "]"}]}], ";"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"u", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"k", "=", "1"}], ",", 
       RowBox[{"k", "<", 
        RowBox[{"n", "+", "1"}]}], ",", 
       RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"cont", "=", "1"}], ";", "\[IndentingNewLine]", 
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Mod", "[", 
            RowBox[{
             RowBox[{"cont", "*", 
              RowBox[{"M", "[", 
               RowBox[{"[", "k", "]"}], "]"}]}], ",", 
             RowBox[{"m", "[", 
              RowBox[{"[", "k", "]"}], "]"}]}], "]"}], ">", "1"}], ",", 
          RowBox[{
           RowBox[{"cont", "=", 
            RowBox[{"cont", "+", "1"}]}], ";"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"u", "[", 
          RowBox[{"[", "k", "]"}], "]"}], "=", "cont"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"tabla", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{"k", "+", "1"}], ",", "5"}], "]"}], "]"}], "=", 
         RowBox[{"u", "[", 
          RowBox[{"[", "k", "]"}], "]"}]}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"b", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"w", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"b", "[", 
       RowBox[{"[", "1", "]"}], "]"}], "=", 
      RowBox[{"Mod", "[", 
       RowBox[{
        RowBox[{"a", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", 
        RowBox[{"m", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"tabla", "[", 
       RowBox[{"[", 
        RowBox[{"2", ",", "7"}], "]"}], "]"}], "=", 
      RowBox[{"b", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"tabla", "[", 
       RowBox[{"[", 
        RowBox[{"2", ",", "6"}], "]"}], "]"}], "=", "\"\<-\>\""}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"f", "=", "2"}], ",", 
       RowBox[{"f", "<", 
        RowBox[{"n", "+", "1"}]}], ",", 
       RowBox[{"f", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"w", "[", 
          RowBox[{"[", "f", "]"}], "]"}], "=", 
         RowBox[{"Mod", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"a", "[", 
               RowBox[{"[", "f", "]"}], "]"}], "-", 
              RowBox[{"b", "[", 
               RowBox[{"[", 
                RowBox[{"f", "-", "1"}], "]"}], "]"}]}], ")"}], "*", 
            RowBox[{"u", "[", 
             RowBox[{"[", "f", "]"}], "]"}]}], ",", 
           RowBox[{"m", "[", 
            RowBox[{"[", "f", "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"tabla", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{"f", "+", "1"}], ",", "6"}], "]"}], "]"}], "=", 
         RowBox[{"w", "[", 
          RowBox[{"[", "f", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"b", "[", 
          RowBox[{"[", "f", "]"}], "]"}], "=", 
         RowBox[{
          RowBox[{"b", "[", 
           RowBox[{"[", 
            RowBox[{"f", "-", "1"}], "]"}], "]"}], "+", 
          RowBox[{
           RowBox[{"w", "[", 
            RowBox[{"[", "f", "]"}], "]"}], "*", 
           RowBox[{"M", "[", 
            RowBox[{"[", "f", "]"}], "]"}]}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"tabla", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{"f", "+", "1"}], ",", "7"}], "]"}], "]"}], "=", 
         RowBox[{"b", "[", 
          RowBox[{"[", "f", "]"}], "]"}]}], ";"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", "\"\<Sistema de congruencias:\>\"", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"f", "=", "1"}], ",", 
       RowBox[{"f", "<", 
        RowBox[{"n", "+", "1"}]}], ",", 
       RowBox[{"f", "++"}], ",", 
       RowBox[{
        RowBox[{"Print", "[", 

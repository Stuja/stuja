         RowBox[{"cadena", "=", 
          RowBox[{"StringJoin", "[", 
           RowBox[{"cadena", ",", "\"\<,\>\""}], "]"}]}]}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"cadena", "=", 
        RowBox[{"StringJoin", "[", 
         RowBox[{"cadena", ",", "cadena1"}], "]"}]}]}]}], "]"}]}], "]"}], 
  ";"}], "\n", 
 RowBox[{"Print", "[", 
  RowBox[{"\"\<A = \>\"", ",", "A"}], "]"}], "\n", 
 RowBox[{"Print", "[", 
  RowBox[{"\"\<B = \>\"", ",", "B"}], "]"}], "\n", 
 RowBox[{"Cartesiano", "=", 
  RowBox[{"ToExpression", "[", 
   RowBox[{"StringJoin", "[", 
    RowBox[{"\"\<{\>\"", ",", "cadena", ",", "\"\<}\>\""}], "]"}], 
   "]"}]}]}], "Input"],


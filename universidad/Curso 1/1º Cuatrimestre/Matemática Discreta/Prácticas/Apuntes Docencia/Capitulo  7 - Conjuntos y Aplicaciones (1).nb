Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"X", " ", "=", " ", 
   RowBox[{"{", 
    RowBox[{"a", ",", "b", ",", "c"}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Partes", " ", "=", " ", 
   RowBox[{"{", 
    RowBox[{"{", "}"}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"j", " ", "=", " ", "1"}], ",", " ", 
    RowBox[{"j", " ", "<=", " ", 
     RowBox[{"Length", "[", "X", "]"}]}], ",", " ", 
    RowBox[{"j", "++"}], ",", "\n", "      ", 
    RowBox[{
     RowBox[{"temp", " ", "=", " ", 
      RowBox[{"Length", "[", "Partes", "]"}]}], ";", "\n", "      ", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
       RowBox[{"i", " ", "<=", " ", "temp"}], ",", " ", 
       RowBox[{"i", "++"}], ",", "\n", "            ", 
       RowBox[{
        RowBox[{"Partes", " ", "=", " ", 
         RowBox[{"Append", "[", 
          RowBox[{"Partes", ",", " ", 
